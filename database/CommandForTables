database web_db;
tables Album,Music,avis,users;



CREATE TABLE users (
email VARCHAR(30) NOT NULL,
password VARCHAR(30) NOT NULL,
pseudo VARCHAR(30),
imagePath VARCHAR(2083),
PRIMARY KEY (email)
)ENGINE=InnoDB 


CREATE TABLE Album (
nomAlbum VARCHAR(30) NOT NULL,
nomArtist VARCHAR(30) NOT NULL,
Annee year(4) DEFAULT NULL,
Label SMALLINT(5) UNSIGNED,
imagePath VARCHAR(120),
)ENGINE=InnoDB 

CREATE TABLE Music (
idMusic int(10) unsigned NOT NULL AUTO_INCREMENT,
Titre varchar(30) DEFAULT NULL,
nomAlbum VARCHAR(30) NOT NULL,
Caract longtext,
musicPath longtext,
dateAjout date,
PRIMARY KEY (idMusic)
)ENGINE=InnoDB 

CREATE TABLE avis (
id bigint(20) unsigned NOT NULL AUTO_INCREMENT,
useremail varchar(30) DEFAULT NULL,
idmusic int(10) NOT NULL,
note smallint(5) unsigned ,
humeur longtext,
PRIMARY KEY (id)
)ENGINE=InnoDB 

